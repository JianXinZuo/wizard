{
  "name": "wizardoc",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@magicspace/configs": "0.1.46",
    "lerna": "^3.20.2",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "client:start": "cd client && yarn start",
    "server:start": "cd server && yarn start:dev",
